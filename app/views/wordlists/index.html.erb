<h1>Listing wordlists</h1>

<table>
  <tr>
    <th>Title</th>
  </tr>

<% @wordlists.each do |wordlist| %>
  <tr>
    <td><%=h wordlist.name %></td>
    <td><%= link_to 'Show', wordlist %></td>
    <td><%= link_to 'Edit', edit_wordlist_path(wordlist) %></td>
    <td><%= link_to 'Destroy', wordlist, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New wordlist', new_wordlist_path %>